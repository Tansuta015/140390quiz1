<% include ../partials/header %>
<div class="jumbotron">
            
<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Title</th>
            <th>Credits</th>
            <th>Sections</th>

        </tr>
    </thead>
    <tbody>
    <%subject.forEach(function(subject){ %>
<tr>
            <td scope="row"><%= subject.id%></td>
            <td><%= subject.title%></td>
            <td><%= subject.credits%></td>
            <td><%= subject.sections%></td>
        </tr>
    <% }); %>
        
        
    </tbody>
</table>

        </div>