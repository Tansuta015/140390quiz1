<% include ../partials/header %>
<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Title</th>
            <th>Firstname</th>
            <th>Lastname</th>

        </tr>
    </thead>
    <tbody>
    <%home.forEach(function(home){ %>
<tr>
            <td scope="row"><%= home.id %></td>
            <td><%= home.title %></td>
            <td><%= home.firstname %></td>
            <td><%= home.lastname %></td>
        </tr>
    <% }); %>
        
        
    </tbody>
</table>
<% include ../partials/footer %>