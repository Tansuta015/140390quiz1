<% include ../partials/header %>
<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Title</th>
            <th>Firstname</th>
            <th>Lastname</th>

        </tr>
    </thead>
    <tbody>
    <%subject.forEach(function(subject){ %>
<tr>
            <td scope="row"><%= subject.id %></td>
            <td><%= subject.title %></td>
            <td><%= subject.firstname %></td>
            <td><%= subject.lastname %></td>
        </tr>
    <% }); %>
        
        
    </tbody>
</table>
<% include ../partials/footer %>